module HLT1Jet = PATHLTProducer {
    InputTag triggerResults  = TriggerResults::HLT
    string   triggerName     = "HLT1Jet"
    InputTag filterName      = hlt1jet200::HLT
}
replace MessageLogger.suppressInfo += "HLT1Jet"

