
# module to filter on the number of Leptons

module countLayer1Leptons = PATLeptonCountFilter {
  InputTag electronSource = selectedLayer1Electrons
  InputTag muonSource     = selectedLayer1Muons
  InputTag tauSource      = selectedLayer1Taus
  bool     countElectrons = true
  bool     countMuons     = true
  bool     countTaus      = false
  uint32   minNumber      = 1
  uint32   maxNumber      = 999999
}
